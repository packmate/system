goToAndClear() {
  cd $1 && clear
}

alias b='tmuxinator basic'
alias d='goToAndClear ~/Desktop'
alias dotfiles='goToAndClear ~/Development/@packmate/system/dotfiles'
alias f='tmuxinator foundation'
alias gr='goToAndClear ~/Development/@thinkful/grading'
alias pe='goToAndClear ~/Development/@praising-earth/praising-earth'
alias v='goToAndClear ~/Development'
